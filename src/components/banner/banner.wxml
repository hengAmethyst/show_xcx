 <swiper class="banner" indicator-dots="{{indicatorDots}}" indicator-active-color="{{indicatorActiveColor}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{itemData.data}}" wx:key="index">
      <swiper-item>
        <image src="{{item.url}}" mode="widthFix" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>